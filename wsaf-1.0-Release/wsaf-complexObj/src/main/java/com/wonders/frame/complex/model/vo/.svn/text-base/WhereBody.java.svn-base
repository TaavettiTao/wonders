package com.wonders.frame.complex.model.vo;

import java.util.List;

import com.wonders.frame.complex.model.vo.and.JointQuerys;

public class WhereBody {

	private List<JointQuerys> jointQuerys;

	// 通用，支持< > <= >= in like
	private List<GeneralQuery> generalQueries;

	private List<OrderBody> orderBodies;
	
	public void setJointQuerys(List<JointQuerys> jointQuerys) {
		this.jointQuerys = jointQuerys;
	}
	public List<GeneralQuery> getGeneralQueries() {
		return generalQueries;
	}
	public void setGeneralQueries(List<GeneralQuery> generalQueries) {
		this.generalQueries = generalQueries;
	}
	public List<OrderBody> getOrderBodies() {
		return orderBodies;
	}
	public void setOrderBodies(List<OrderBody> orderBodies) {
		this.orderBodies = orderBodies;
	}


}
